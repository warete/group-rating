RewriteEngine on
RewriteBase /

# Страницы.
RewriteRule ^semestr/([0-9]+)/?$	semestr.php?sem=$1
RewriteRule ^semestr/([0-9]+)/([A-Za-z0-9-]+)/([A-Za-z0-9-]+)/?$	semestr.php?sem=$1&sort=$2&direction=$3
RewriteRule ^materials/ index.php?action=materials
RewriteRule ^student/([0-9]+)/([0-9]+)/?$	index.php?student=$1&semestr=$2
RewriteRule ^student/([0-9]+)?$	index.php?student=$1
